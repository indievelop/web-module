  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "flow": "flow",
    "start-js": "node scripts/start.js",
    "start": "npm run start-js",
    "build": "npm run react-scripts build",
    "test": "node scripts/test.js --env=jsdom",
    "tslint": "tslint -c tslint.json 'src/**/.*.{js,jsx,ts,tsx,mjs}'"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(js|jsx|ts|tsx|mjs)$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "mjs",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
